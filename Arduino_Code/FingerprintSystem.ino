//---------------------------------------------------------------------------------------
void setup() {
  Serial.begin(115200);
  // initialize the OLED object
  if(!display.begin(SSD1306_SWITCHCAPVCC, SCREEN_ADDRESS)) {
    Serial.println(F("SSD1306 allocation failed"));
    for(;;); // Don't proceed, loop forever
  }
  // Clear the buffer.
  display.clearDisplay();
//---------------------------------------------------------------------------------------
  WiFiConnection();
//---------------------------------------------------------------------------------------

  finger.begin(57600);
  Serial.println("\n\nFingeprint Attendance System!");
  Serial.println("\nAdafruit Fingeprint Detect Test");

  if (finger.verifyPassword()) {
    Serial.println("Found Fingerprint Sensor!");
    display.clearDisplay();
    display.drawBitmap(34, 0, Fingerprint_valid_bits, Fingerprint_valid_width, Fingerprint_valid_height, WHITE);
    display.display();
  } else {
    Serial.println("Fingerprint sensor not found :(");
    display.clearDisplay();
    display.drawBitmap(32, 0, Fingerprint_invalid_bits, Fingerprint_invalid_width, Fingerprint_invalid_height, WHITE);
    display.display();
    while (1) {
      delay(1);
    }
  }
//---------------------------------------------------------------------------------------------------------------------
  finger.getTemplateCount();
  Serial.print("Sensor contains ");
  Serial.print(finger.templateCount);
  Serial.println(" templates");

  Serial.println("Waiting for valid finger...");
//-----------------------------------------------
  timer.setInterval(25000L, modeCheck);
  t1 = timer.setInterval(10000L, checkToAdd);
  t2 = timer.setInterval(15000L, checkToDelete);
//-----------------------------------------------
  modeCheck();
}
//-----------------------------------------------
void loop() {
  timer.run();
  
  if (WiFi.status() != WL_CONNECTED) {
    if (millis() - previousMillis >= 10000) {
      previousMillis = millis();
      WiFiConnection();
    }
  }
  checkFingerprints();
  delay(20);
}

void checkFingerprints() {
  FingerID = getFingerprintID();
  displayFingerprints();
}
//-----------------------Fingerprint ID state displayed on OLED-------------------------
void displayFingerprints() {
//Fingerprint detected-------------------------
  if (FingerID > 0) {
    display.clearDisplay();
    display.drawBitmap(34, 0, Fingerprint_valid_bits, Fingerprint_valid_width, Fingerprint_valid_height, WHITE);
    display.display();
    sendFingerID(FingerID);
    delay(2000);
  } 
//No Finger Detected-------------------------
  else if (FingerID == 0) {
    display.clearDisplay();
    display.drawBitmap(34, 0, Fingerprint_scan_bits, Fingerprint_scan_width, Fingerprint_scan_height, WHITE);
    display.display();
  }
//No Match Found-------------------------
  else if (FingerID == -1) {
    display.clearDisplay();
    display.drawBitmap(34, 0, Fingerprint_invalid_bits, Fingerprint_invalid_width, Fingerprint_invalid_height, WHITE);
    display.display();
  }
//Unknown error-------------------------
  else if (FingerID == -2) {
    display.clearDisplay();
    display.drawBitmap(34, 0, Fingerprint_failed_bits, Fingerprint_failed_width, Fingerprint_failed_height, WHITE);
    display.display();
  }
}


void sendFingerID(int finger) {
  Serial.println("Sending Fingeprint ID");
  if (WiFi.status() == WL_CONNECTED) {
    getData = "?FingerID=" + String(finger) + "&device_token=" + String(token);
    Link = URL + getData;

    http.get(Link);
    int httpCode = http.responseStatusCode();
    String payload = http.responseBody();

    Serial.println(httpCode);
    Serial.println(payload);
    Serial.println(finger);

    if (payload.startsWith("login")) {
      String username = payload.substring(5);
      display.clearDisplay();
      display.setTextSize(2);
      display.setTextColor(WHITE);
      display.setCursor(25,10);
      display.print(F("Welcome"));
      display.setCursor(25,30);
      display.print(username);
      display.display();
    }
    else if (payload.startsWith("logout")) {
      String username = payload.substring(5);
      display.clearDisplay();
      display.setTextSize(2);
      display.setTextColor(WHITE);
      display.setCursor(0,10);
      display.print(F("Bye"));
      display.setCursor(-12,30);
      display.print(username);
      display.display();
    }
    delay(10);
    http.stop();
  }
}

